<!--*******************************************************-->
<!--            Post service action                        -->
<!--*******************************************************-->
	<method name="POST" id="postServiceAction">
		<wadl:doc xml:lang="EN" title="Post service action"
			xmlns="http://docbook.org/ns/docbook">
			<para role="shortdesc">Changes the operator status of, or enables, diables, or deletes services for a <code>project_id</code> or a domain.</para>
			<para>To change the operator status of or delete services for a <code>project_id</code> or a domain, provide a JSON request body with <code>action</code> set to <code>enable</code>, <code>disable</code>, or <code>delete</code>, along with the <code>project_id</code> of the user or the <code>domain</code>. </para>			
		</wadl:doc>
		<request>
			<representation mediaType="application/json" >
				<doc>
					<xsdxt:code href="../wadl/samples/cdn-postServiceAction_request-json-http.txt"/>
					<xsdxt:code href="../wadl/samples/cdn-postServiceAction_request.json"/>
				</doc>
				
				<param name="project_id" style="plain" required="true" path="$.project_id">
					<wadl:doc xmlns="http://docbook.org/ns/docbook"
						xml:lang="EN">Specifies the associated identifier for the user account, such as, 12345.</wadl:doc>
				</param>
				
				<param name="action" style="plain" required="true" path="$.action">
					<wadl:doc xmlns="http://docbook.org/ns/docbook"
						xml:lang="EN">Specifies the action to perfrom, such as <code>enable</code>, <code>disable</code>, or <code>delete</code>. </wadl:doc>
				</param>
			</representation>
			<representation mediaType="application/json" >
				
				<doc>
					<xsdxt:code href="../wadl/samples/cdn-postServiceAction_request-json-http.txt"/>
					<xsdxt:code href="../wadl/samples/cdn-postServiceActionWithDomain_request.json"/>
				</doc>
				
				<param name="domain" style="plain" required="true" path="$.domain">
					<wadl:doc xmlns="http://docbook.org/ns/docbook"
						xml:lang="EN">Specifies a string representing the domain, such as <code>www.example.com</code>.</wadl:doc>
				</param>
				<param name="action" style="plain" required="true" path="$.action">
					<wadl:doc xmlns="http://docbook.org/ns/docbook"
						xml:lang="EN">Specifies the action to perfrom, such as <code>enable</code>, <code>disable</code>, or <code>delete</code>. </wadl:doc>
				</param>
			</representation>
		</request>
		<response status="202" xmlns="http://wadl.dev.java.net/2009/02">
			<doc title="Accepted" xml:lang="EN">The request has been accepted for processing.</doc>
			<representation mediaType="application/json">
				<wadl:doc xmlns="http://docbook.org/ns/docbook"
					xml:lang="EN">
					<xsdxt:code
						href="../wadl/samples/cdn-postServiceAction_response-json-http.txt"/>	 	 			
				</wadl:doc>
			</representation>
		</response>		
	</method>

