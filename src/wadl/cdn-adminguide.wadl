	<method name="POST" id="postAkamaiBackground">
		<wadl:doc xml:lang="EN" title="Run Akamai-related background job"
			xmlns="http://docbook.org/ns/docbook">
			<para role="shortdesc">Runs Akamai-related background job.</para>
			<para>This operation runs an Akamai-related background job.</para>
			<note><para>The following types of jobs are supported:
				<itemizedlist>
					<listitem><para>akamai_update_papi_property_for_mod_san</para></listitem>
					<listitem><para>akamai_check_and_update_cert_status</para></listitem>
				</itemizedlist>
				</para></note>
		</wadl:doc>
		<request>
			<representation mediaType="application/json" >
				<doc>
					<xsdxt:code href="../wadl/samples/cdn-postAkamaiBackground_request-json-http.txt"/> 
					<xsdxt:code href="../wadl/samples/cdn-postAkamaiBackground_request.json"/>
				</doc>
				<param name="job_type" style="plain" required="true" path="$.job_type">
					<wadl:doc xmlns="http://docbook.org/ns/docbook"
						xml:lang="EN">Specifies the type of background job. The following types of jobs are supported: <code>akamai_update_papi_property_for_mod_san</code> and <code>akamai_check_and_update_cert_status</code>.</wadl:doc>
				</param>
				<param name="updated_cname_host_mapping_info" style="plain" required="true" path="$.updated_cname_host_mapping_info">
					<wadl:doc xmlns="http://docbook.org/ns/docbook"
						xml:lang="EN">When <code>job_type</code> is <code>akamai_update_papi_property_for_mod_san</code>, the list is required and specifies the domain-to-SAN-certificate CNAME mapping.</wadl:doc>
				</param>
				<param name="domain_name" style="plain" required="true" path="$.updated_cname_host_mapping_info.domain_name">
					<wadl:doc xmlns="http://docbook.org/ns/docbook"
						xml:lang="EN">Specifies a string representing the customer's domain name, such as <code>testabc.cnamecdn.com</code>.</wadl:doc>
				</param>
				<param name="san_cert_name" style="plain" required="true" path="$.updated_cname_host_mapping_info.san_cert_name">
					<wadl:doc xmlns="http://docbook.org/ns/docbook"
						xml:lang="EN">Specifies the SAN certificate CNAME domain to which the customer is mapping, for example, <code>secure1.san1.altcdn.com</code>. </wadl:doc>
				</param>
				<param name="project_id" style="plain" required="false" path="$.updated_cname_host_mapping_info.project_id">
					<wadl:doc xmlns="http://docbook.org/ns/docbook"
						xml:lang="EN">When <code>job_type</code> is <code>akamai_check_and_update_cert_status</code>, specifies the customer's project ID. </wadl:doc>
				</param>
				<param name="cert_type" style="plain" required="false" path="$.updated_cname_host_mapping_info.cert_type">
					<wadl:doc xmlns="http://docbook.org/ns/docbook"
						xml:lang="EN">When <code>job_type</code> is <code>akamai_check_and_update_cert_status</code>, specifies the customer's certificate type. Currently, the only valid value is <code>san</code>. </wadl:doc>
				</param>
				<param name="flavor_id" style="plain" required="false" path="$.updated_cname_host_mapping_info.flavor_id">
					<wadl:doc xmlns="http://docbook.org/ns/docbook"
						xml:lang="EN">When <code>job_type</code> is <code>akamai_check_and_update_cert_status</code>, specifies the customer's service flavor. </wadl:doc>
				</param>
				<param name="action" style="plain" required="false" path=".action">
					<wadl:doc xmlns="http://docbook.org/ns/docbook"
						xml:lang="EN">When the <code>job_type</code> is <code>akamai_update_papi_property_for_mod_san</code>, valid values for <code>action</code> are <code>add</code> or <code>remove</code>. </wadl:doc>
				</param>
			</representation>
			
		</request>
		<response status="202" xmlns="http://wadl.dev.java.net/2009/02">
			<doc title="Accepted" xml:lang="EN">The request has been accepted for processing.</doc>
			<representation mediaType="application/json">
				<wadl:doc xmlns="http://docbook.org/ns/docbook"
					xml:lang="EN">
					<xsdxt:code href="../wadl/samples/cdn-postAkamaiBackground_response-json-http.txt"/>
					
				</wadl:doc>
			</representation>
		</response>		
	</method>



