<!--*******************************************************-->
<!--   Query services and project IDs by service status    -->
<!--*******************************************************-->
	<method name="GET" id="getServiceProjectIDsByStatus">
		<wadl:doc xml:lang="EN" title="Get service IDs and project IDs by status "
			xmlns="http://docbook.org/ns/docbook">
			<para role="shortdesc">Get services and project IDs by <code>status</code>.</para>
			<para> To get services and project IDs by <code>status</code>, provide <code>status</code> as a query parameter in the endpoint URL of the operation. 
				Valid values for <code>status</code> are <code>deployed</code>,  <code>failed</code>, or 
				<code>update_in_progress</code>. No JSON request body is required. </para>			
		</wadl:doc>
		<request>
			<param name="status" style="query" type="xsd:string"
				required="true">
				<wadl:doc xmlns="http://docbook.org/ns/docbook"
					xml:lang="EN">Specifies the status for which you want to list services and 
					project IDs. Valid values for <code>status</code> are <code>deployed</code>,  <code>failed</code>,  
					<code>update_in_progress</code>, <code>deploy_in_progress</code>, and <code>delete_in_progress</code>.</wadl:doc>
			</param>
			<representation mediaType="application/json" >
				<doc>
					<xsdxt:code href="../wadl/samples/cdn-getServiceProjectIDs_request-json-http.txt"/>  
				</doc>
			</representation>
		</request>
		<response status="200" xmlns="http://wadl.dev.java.net/2009/02">
			<doc title="OK" xml:lang="EN">Success.</doc>
			<representation mediaType="application/json">
				<wadl:doc xmlns="http://docbook.org/ns/docbook"
					xml:lang="EN">
					<xsdxt:code
						href="../wadl/samples/cdn-getServiceProjectIDs_response-json-http.txt"/> 
					<xsdxt:code 
						href="../wadl/samples/cdn-getServiceProjectIDs_response.json"/>	  
				</wadl:doc>
				<param name="project_id" style="plain" path="$.project_id">
					<wadl:doc xmlns="http://docbook.org/ns/docbook"
						xml:lang="EN">Specifies the project ID for the service.</wadl:doc>
				</param>
				<param name="service_id" style="plain" path="$.service_id">
					<wadl:doc xmlns="http://docbook.org/ns/docbook"
						xml:lang="EN">Specifies the servic ID for the service.</wadl:doc>
				</param>
			</representation>
		</response>		
	</method>	
	
	
<!--*******************************************************-->
<!--            Post service status                        -->
<!--*******************************************************-->
	<method name="POST" id="postServiceStatus">
		<wadl:doc xml:lang="EN" title="Set the status of a service"
			xmlns="http://docbook.org/ns/docbook">
			<para role="shortdesc">Changes the status of a service.</para>
			<para>To change the status of a service for a given <code>service_id</code> and <code>project_id</code>, provide a JSON request body with <code>project_id</code> set to the project ID of the service, <code>serice_id</code> set to the corresponding <code>service_id</code>, and <code>status</code> set to <code>deployed</code> or <code>failed</code>. </para>			
		</wadl:doc>
		<request>
			<representation mediaType="application/json" >
				<doc title="Post service deployed status: JSON request">
					<xsdxt:code href="../wadl/samples/cdn-postServiceStatus_request-json-http.txt"/> 
					<xsdxt:code href="../wadl/samples/cdn-postServiceStatusDeployed_request.json"/>
				</doc>
				
				<param name="project_id" style="plain" required="true" path="$.project_id">
					<wadl:doc xmlns="http://docbook.org/ns/docbook"
						xml:lang="EN">Specifies the associated identifier for the user account, such as, 12345.</wadl:doc>
				</param>
				<param name="serivce_id" style="plain" required="true" path="$.service_id">
					<wadl:doc xmlns="http://docbook.org/ns/docbook"
						xml:lang="EN">Specifies the ID for the service.  </wadl:doc>
				</param>
				<param name="status" style="plain" required="true" path="$.status">
					<wadl:doc xmlns="http://docbook.org/ns/docbook"
						xml:lang="EN">Specifies the status for the service. Valid values are <code>deployed</code> or <code>failed</code>. </wadl:doc>
				</param>
			</representation>
			<representation mediaType="application/json" >
				
				<doc title="Post service failed status: JSON request">
					<xsdxt:code href="../wadl/samples/cdn-postServiceStatus_request-json-http.txt"/>
					<xsdxt:code href="../wadl/samples/cdn-postServiceStatusFailed_request.json"/> 
				</doc>
<!--				
				<param name="project_id" style="plain" required="true" path="$.project_id">
					<wadl:doc xmlns="http://docbook.org/ns/docbook"
						xml:lang="EN">Specifies the associated identifier for the user account, such as, 12345.</wadl:doc>
				</param>
				<param name="serivce_id" style="plain" required="true" path="$.service_id">
					<wadl:doc xmlns="http://docbook.org/ns/docbook"
						xml:lang="EN">Specifies the ID for the service.  </wadl:doc>
				</param>
				<param name="status" style="plain" required="true" path="$.status">
					<wadl:doc xmlns="http://docbook.org/ns/docbook"
						xml:lang="EN">Specifies the status for the service. Valid values are <code>deployed</code> or <code>failed</code>. </wadl:doc>
				</param> -->
			</representation>
		</request>
		<response status="201" xmlns="http://wadl.dev.java.net/2009/02">
			<doc title="Created" xml:lang="EN">Following a POST command, this status indicates success. The request has been fulfilled.</doc>
			<representation mediaType="application/json">
				<wadl:doc xmlns="http://docbook.org/ns/docbook"
					xml:lang="EN">
					<xsdxt:code
						href="../wadl/samples/cdn-postServiceStatus_response-json-http.txt"/>	 	  			
				</wadl:doc>
			</representation>
		</response>		
	</method>


