	<!--*******************************************************-->
	<!--      Set limits on services for a user account        -->
	<!--*******************************************************-->
	<method name="PUT" id="putLimitsProjectID">
		<wadl:doc xml:lang="EN" title="Set limits on services for a user account"
			xmlns="http://docbook.org/ns/docbook">
			<para role="shortdesc">Sets limits on the number of services.</para>
			<para>To set limits on the number of services for a particular <code>project_id</code>, provide a JSON request body with <code>limit</code> set to a positive integer. Provide the <code>project_id</code> associated with the user account in the endpoint URL.</para>			
		</wadl:doc>
		<request>
			<representation mediaType="application/json" >
				<doc>
					<xsdxt:code href="../wadl/samples/cdn-putLimitsProjectID_request-json-http.txt"/>
					<xsdxt:code href="../wadl/samples/cdn-putLimitsProjectID_request.json"/>	
				</doc>
				<param name="limit" style="plain" required="true" path="$.limit">
					<wadl:doc xmlns="http://docbook.org/ns/docbook"
						xml:lang="EN">Specifies the limit on the number of services for the <code>project_id</code>.</wadl:doc>
				</param>
			</representation>
		</request>
		<response status="201" xmlns="http://wadl.dev.java.net/2009/02">
			<doc title="Created" xml:lang="EN">The request has been fulfilled.</doc>
			<representation mediaType="application/json">
				<wadl:doc xmlns="http://docbook.org/ns/docbook"
					xml:lang="EN">
					<xsdxt:code
						href="../wadl/samples/cdn-putLimitsProjectID_response-json-http.txt"/> 	 	 			
				</wadl:doc>
			</representation>
		</response>		
	</method>
	
	<!--*******************************************************-->
	<!--      Get limits on services for a user account        -->
	<!--*******************************************************-->
	<method name="GET" id="getLimitsProjectID">
		<wadl:doc xml:lang="EN" title="Get limits on services for a user account"
			xmlns="http://docbook.org/ns/docbook">
			<para role="shortdesc">Get limits on the number of services.</para>
			<para> To get limits on the number of services for a particular <code>project_id</code>, provide the <code>project_id</code> in the endpoint URL of the operation. No JSON request body is required. </para>			
		</wadl:doc>
		<request>
			<representation mediaType="application/json" >
				<doc>
					<xsdxt:code href="../wadl/samples/cdn-getLimitsProjectID_request-json-http.txt"/> 
				</doc>
			</representation>
		</request>
		<response status="200" xmlns="http://wadl.dev.java.net/2009/02">
			<doc title="OK" xml:lang="EN">Success.</doc>
			<representation mediaType="application/json">
				<wadl:doc xmlns="http://docbook.org/ns/docbook"
					xml:lang="EN">
					<xsdxt:code
						href="../wadl/samples/cdn-getLimitsProjectID_response-json-http.txt"/>
					<xsdxt:code href="../wadl/samples/cdn-getLimitsProjectID_response.json"/>	  
				</wadl:doc>
				<param name="limit" style="plain" required="true" path="$.limit">
					<wadl:doc xmlns="http://docbook.org/ns/docbook"
						xml:lang="EN">Specifies the limit on the number of services for the <code>project_id</code>.</wadl:doc>
				</param>
			</representation>
		</response>		
	</method>
	
	<!--*******************************************************-->
